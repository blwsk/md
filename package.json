{
  "name": "md",
  "version": "0.0.1",
  "scripts": {
    "buckle:dev": "bsb -make-world -w",
    "next:dev": "node ./server/index.js",
    "dev": "concurrently \"npm run buckle:dev\" \"npm run next:dev\"",
    "build": "next build",
    "start": "next start -w"
  },
  "license": "MIT",
  "dependencies": {
    "babel-plugin-bucklescript": "^0.2.3-1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "bs-platform": "^2.1.0",
    "concurrently": "^3.5.0",
    "next": "^4.0.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-markdown": "^3.0.1",
    "reason-react": "^0.3.0"
  },
  "babel": {
    "presets": ["next/babel"],
    "plugins": ["babel-plugin-bucklescript"]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
